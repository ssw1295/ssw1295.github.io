---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

    {% for entry in site.entry %}
    <url>
        <loc>https://ssw1295.github.io{{ entry.url | replace: 'index.html', '' }}</loc>
        <lastmod>{{ entry.date | default: site.time | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.5</priority>
    </url>
    {% endfor %}

    {% for page in site.pages %}
    {% if page.layout != nil and page.layout != 'feed' %}
    <url>
        <loc>https://ssw1295.github.io{{ page.url | replace: 'index.html', '' }}</loc>
        <lastmod>{{ page.date | default: site.time | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.5</priority>
    </url>
    {% endif %}
    {% endfor %}

</urlset>